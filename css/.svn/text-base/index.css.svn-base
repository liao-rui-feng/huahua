/*------------ banner ------------- */


#banner .lbanner{
width:200px;
height:420px;
background-color: #f3f3f3;
position:relative;


}

#banner .rbanner{

width:1000px;
height:420px;

}

#banner .lbanner li{
    line-height:28px;
    border:1px solid transparent;
    
}

#banner .lbanner li p{
padding-left:18px;

}

#banner .lbanner li a{
   color:#1a1a1a;
    
    }


/* 左侧导航Hover */

#banner .ul_hover{
width:578px;
height:418px;
background-color: #fff;
position:absolute;
left:200px;
top:0px;
z-index:101;
display:none;
border:1px solid #f60;
}

#banner .lbanner li:hover{
background-color: #fff;
border:1px solid #f60;
border-right:none;

}

#banner .lbanner li:hover p{
   width:184px;
   position:relative;
   z-index:102;  
   background-color: white;

}

#banner .lbanner li:hover .ul_hover{
    display:block;   
 }

 /* 今日推荐样式 */

.box_title{
     line-height:33px;
     font-size:24px;
     color:#f60;
     border-bottom:6px solid #f60;
     text-indent:17px;
     margin-top:13px;
 }

 #recommend  .recommend_box{
    width:1188px;
    margin:0 auto;
    border-left:1px solid #f60;
 }

 #recommend  .recommend_box li{
    width:198px;
    height:248px;
    float:left;
    border-right:1px solid #f60;
    border-bottom:1px solid #f60;
    box-sizing: border-box;
    
 }

 #recommend  .recommend_box li div{

    padding:0 7px;
 }

 #recommend  .recommend_box li img:first-child{
margin-left:25px;

 }

 /* 电子书 */

 #ebook{
     margin-top:36px;
 }

 #ebook .ebook_left{
    width:904px;
    position:relative;

 }

 .hover_title{
    
    line-height:33px;
    border-bottom:6px solid #f60; 
}

.title_hover{
   
    background-color: #f60;
    color:white;
}

 .hover_title p{
    width:100px;
    text-align:center;
} 
/* 电子书，左侧盒子，左侧内容 */
#ebook .ebook_left .ebook_left_box1{
width:238px;
height:441px;


} 

#ebook .ebook_left .ebook_left_box1 ul{
    
    line-height:39px;
    width:79px;
    background-color: #f9f9f9;
    height:152px;
}



#ebook .ebook_left .ebook_left_box1 a,#cloth .cloth_left a{
    color:#333;
}

/* 电子书，左侧盒子，右侧内容 */
#ebook .ebook_left .ebook_left_box2{
    width:666px;
    border-right:1px solid #ccc;
    box-sizing: border-box;

}

.ebook_left_box2 .li_1{
    width:332px;
    height:220px;
    box-sizing: border-box;
    border-left: 1px solid #ccc;
    border-bottom:1px solid #ccc;
}

.ebook_left_box2 .li_2{
    width:166px;
    height:220px;
    box-sizing: border-box;
    border-left: 1px solid #ccc;
    border-bottom:1px solid #ccc;
    padding:12px 0 0px 5px;
    line-height:22px;
}


/* 电子书右侧 */
#ebook .ebook_right{
    width:284px;
    height:440px;
    border:1px #ccc solid;
    box-sizing: border-box;
    margin-top:39px;

}

#ebook .ebook_right h2{
    line-height:37px;
}

#ebook .ebook_right ul{
    padding-left:22px;
}

#ebook .ebook_right ul li{
    position:relative;
    line-height:40px;
    border-bottom:1px solid #ccc;
    font-size:16px;
    text-indent:9px;
    color:#4d4d4d;
    

}
#ebook .ebook_right ul li:last-child{
    border-bottom:0px ;
  
}

#ebook .ebook_right ul span{
    position:absolute;
    top:0px;
    left:-20px;
    color:red;
    
}

.newbook{
    width:126px;
    line-height:18px;
    font-size:14px;
    text-indent:0px;
    text-align:justify;
    margin-top:6px;
    margin-right:6px;
    
}

.newbook_hov1{
    line-height:0px;
}

/* 电子书左侧选项卡 */
.left_hov2,.left_hov3,.left_hov1{
    position:absolute;
