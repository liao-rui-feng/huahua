/* 左侧楼层导航 */
.floor{
    width:40px;
    position:fixed;
    top:50%;
    left:50%;
    transform: translateY(-50%);
    height:200px;
    margin-left:-640px;
   
   

}
.floor li{
    height:40px;
    padding-left:40px;
    background: url(../imgs/icon.png) no-repeat;
   line-height:40px;
   text-align:center;
   color:white;
   overflow:hidden;
   cursor: pointer;
}

.floor .icon2{
    background-position-y:-80px;
}

.floor .icon3{
    background-position-y:-160px;
}

.floor .icon4{
    background-position-y:-200px;
}

.floor .icon5{
    background-position-y:-320px;
}


